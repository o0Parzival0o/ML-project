          "patience": 50,
          "monitor": "val_loss",
          "epsilon_loss_down": 0.000001,
          "epsilon_accuracy_up": 0.005,
          "target_loss": 0.0
      },
      "splitting": {
          "tr": 0.75,
          "vl": 0.15,
          "ts": 0.10
      }
  },
  "initialization": {
      "method": "fan_in"
  },
  "paths": {
      "train_data": "../../MONK files/monks-1.train",
      "test_data": "../../MONK files/monks-1.test"
  },
  "preprocessing": {
      "type": null,
